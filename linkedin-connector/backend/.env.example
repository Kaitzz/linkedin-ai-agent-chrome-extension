# LinkedIn Connector Backend Configuration

# Django Secret Key (change in production)
SECRET_KEY=your-secret-key-here

# Debug mode (set to False in production)
DEBUG=True

# Allowed hosts
ALLOWED_HOSTS=localhost,127.0.0.1

# Database (leave empty to use SQLite)
# DB_NAME=linkedin_connector
# DB_USER=postgres
# DB_PASSWORD=your-password
# DB_HOST=localhost
# DB_PORT=5432

# AI API Keys (choose one)

# Groq (recommended - fast and free)
# Get your key at: https://console.groq.com
GROQ_API_KEY=your-groq-api-key-here

# OpenAI (legacy)
# OPENAI_API_KEY=sk-...

# Gemini (legacy)
# GEMINI_API_KEY=AIza...
